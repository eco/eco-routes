{
  "branches": [
    {
      "name": "main",
      "channel": "beta"
    },
    {
      "name": "clean-semantic-release",
      "channel": "beta"
    }
  ],
  "repositoryUrl": "git@github.com:eco/eco-routes.git",
  "lastRelease": {
    "version": "2.1.5",
    "gitTag": "v2.1.5",
    "gitHead": "1c1fdd412f40a7d2e13ac357173da73fe1416559"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "docs",
            "scope": "README",
            "release": "patch"
          },
          {
            "type": "chain",
            "release": "patch"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "breaking",
            "release": "major"
          }
        ],
        "parserOpts": {
          "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES"]
        }
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "./dist/semantic-release/index.js",
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "CHANGELOG.md",
          "build/deployAddresses.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
}
